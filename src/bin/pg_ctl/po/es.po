#: pg_ctl.c:199 pg_ctl.c:214 pg_ctl.c:1437
#, c-format
msgid "%s: out of memory\n"
msgstr "%s: memoria agotada\n"

#: pg_ctl.c:248
#, c-format
msgid "%s: could not open PID file \"%s\": %s\n"
msgstr "%s: no se pudo abrir el archivo de PID «%s»: %s\n"

#: pg_ctl.c:255
#, c-format
msgid "%s: invalid data in PID file \"%s\"\n"
msgstr "%s: datos no válidos en archivo de PID «%s»\n"

#: pg_ctl.c:501
#, c-format
msgid ""
"%s: another postmaster may be running; trying to start postmaster anyway\n"
msgstr ""
"%s: otro postmaster puede estar en ejecución;\n"
"tratando de ejecutarlo de todas formas.\n"

#: pg_ctl.c:519
#, c-format
msgid "%s: could not read file \"%s\"\n"
msgstr "%s: no se pudo leer el archivo «%s»\n"

#: pg_ctl.c:525
#, c-format
msgid "%s: option file \"%s\" must have exactly one line\n"
msgstr "%s: archivo de opciones «%s» debe tener exactamente una línea\n"

#: pg_ctl.c:584
#, c-format
msgid ""
"The program \"postmaster\" is needed by %s but was not found in the\n"
"same directory as \"%s\".\n"
"Check your installation.\n"
msgstr ""
"%s necesita el programa «postmaster», pero no se encontró\n"
"en el mismo directorio que %s.\n"
"Verifique su instalación.\n"

#: pg_ctl.c:590
#, c-format
msgid ""
"The program \"postmaster\" was found by \"%s\"\n"
"but was not the same version as %s.\n"
"Check your installation.\n"
msgstr ""
"%s encontró el programa «postmaster» pero no corresponde\n"
"a la misma versión que %s.\n"
"Verifique su instalación.\n"

#: pg_ctl.c:602
#, c-format
msgid "%s: could not start postmaster: exit code was %d\n"
msgstr "%s: no se pudo iniciar postmaster: el código de retorno fue %d\n"

#: pg_ctl.c:613
#, c-format
msgid ""
"%s: could not start postmaster\n"
"Examine the log output.\n"
msgstr ""
"%s: no se pudo ejecutar postmaster.\n"
"Examine el registro del servidor.\n"

#: pg_ctl.c:622
msgid "waiting for postmaster to start..."
msgstr "esperando que postmaster se inicie..."

#: pg_ctl.c:626
#, c-format
msgid "could not start postmaster\n"
msgstr "no se pudo iniciar postmaster\n"

#: pg_ctl.c:631 pg_ctl.c:697 pg_ctl.c:770
msgid " done\n"
msgstr " listo\n"

#: pg_ctl.c:632
msgid "postmaster started\n"
msgstr "postmaster iniciado\n"

#: pg_ctl.c:636
msgid "postmaster starting\n"
msgstr "postmaster iniciándose\n"

#: pg_ctl.c:650 pg_ctl.c:718 pg_ctl.c:792
#, c-format
msgid "%s: PID file \"%s\" does not exist\n"
msgstr "%s: el archivo de PID «%s» no existe\n"

#: pg_ctl.c:651 pg_ctl.c:720 pg_ctl.c:793
msgid "Is postmaster running?\n"
msgstr "¿Está postmaster en ejecución?\n"

#: pg_ctl.c:657
#, c-format
msgid "%s: cannot stop postmaster; postgres is running (PID: %ld)\n"
msgstr ""
"%s: no se puede detener postmaster, postgres está en ejecución (PID: %ld)\n"

#: pg_ctl.c:665 pg_ctl.c:742
#, c-format
msgid "%s: could not send stop signal (PID: %ld): %s\n"
msgstr "%s: falló la señal de detención (PID: %ld): %s\n"

#: pg_ctl.c:672
msgid "postmaster shutting down\n"
msgstr "postmaster deteniéndose\n"

#: pg_ctl.c:677 pg_ctl.c:747
msgid "waiting for postmaster to shut down..."
msgstr "esperando que postmaster se detenga..."

#: pg_ctl.c:692 pg_ctl.c:764
msgid " failed\n"
msgstr " falló\n"

#: pg_ctl.c:694 pg_ctl.c:766
#, c-format
msgid "%s: postmaster does not shut down\n"
msgstr "%s: postmaster no se detiene\n"

#: pg_ctl.c:699 pg_ctl.c:771
#, c-format
msgid "postmaster stopped\n"
msgstr "postmaster detenido\n"

#: pg_ctl.c:721 pg_ctl.c:777
msgid "starting postmaster anyway\n"
msgstr "iniciando postmaster de todas maneras\n"

#: pg_ctl.c:730
#, c-format
msgid "%s: cannot restart postmaster; postgres is running (PID: %ld)\n"
msgstr ""
"%s: no se puede reiniciar postmaster, postgres está en ejecución (PID: %ld)\n"

#: pg_ctl.c:733 pg_ctl.c:802
msgid "Please terminate postgres and try again.\n"
msgstr "Por favor termine postgres e intente nuevamente.\n"

#: pg_ctl.c:775
#, c-format
msgid "%s: old postmaster process (PID: %ld) seems to be gone\n"
msgstr "%s: el proceso postmaster antiguo (PID: %ld) parece no estar\n"

#: pg_ctl.c:799
#, c-format
msgid "%s: cannot reload postmaster; postgres is running (PID: %ld)\n"
msgstr ""
"%s: no se puede recargar postmaster; postgres está en ejecución (PID: %ld)\n"

#: pg_ctl.c:808
#, c-format
msgid "%s: could not send reload signal (PID: %ld): %s\n"
msgstr "%s: la señal de recarga falló (PID: %ld): %s\n"

#: pg_ctl.c:813
msgid "postmaster signaled\n"
msgstr "se ha enviado una señal a postmaster\n"

#: pg_ctl.c:857
#, c-format
msgid "%s: a standalone backend \"postgres\" is running (PID: %ld)\n"
msgstr ""
"%s: un proceso servidor autónomo «postgres» está en ejecución (PID: %ld)\n"

#: pg_ctl.c:869
#, c-format
msgid "%s: postmaster is running (PID: %ld)\n"
msgstr "%s: postmaster está en ejecución (PID: %ld)\n"

#: pg_ctl.c:880
#, c-format
msgid "%s: neither postmaster nor postgres running\n"
msgstr "%s: postmaster o postgres no está en ejecución\n"

#: pg_ctl.c:891
#, c-format
msgid "%s: could not send signal %d (PID: %ld): %s\n"
msgstr "%s: no se pudo enviar la señal %d (PID: %ld): %s\n"

#: pg_ctl.c:925
#, c-format
msgid "%s: could not find own program executable\n"
msgstr "%s: no se pudo encontrar el propio ejecutable\n"

#: pg_ctl.c:934
#, c-format
msgid "%s: could not find postmaster program executable\n"
msgstr "%s: no se pudo encontrar el ejecutable de postmaster\n"

#: pg_ctl.c:988 pg_ctl.c:1020
#, c-format
msgid "%s: could not open service manager\n"
msgstr "%s: no se pudo abrir el gestor de servicios\n"

#: pg_ctl.c:994
#, c-format
msgid "%s: service \"%s\" already registered\n"
msgstr "%s: el servicio «%s» ya está registrado\n"

#: pg_ctl.c:1005
#, c-format
msgid "%s: could not register service \"%s\": error code %d\n"
msgstr "%s: no se pudo registrar el servicio «%s»: código de error %d\n"

#: pg_ctl.c:1026
#, c-format
msgid "%s: service \"%s\" not registered\n"
msgstr "%s: el servicio «%s» no ha sido registrado\n"

#: pg_ctl.c:1033
#, c-format
msgid "%s: could not open service \"%s\": error code %d\n"
msgstr "%s: no se pudo abrir el servicio «%s»: código de error %d\n"

#: pg_ctl.c:1040
#, c-format
msgid "%s: could not unregister service \"%s\": error code %d\n"
msgstr "%s: no se pudo dar de baja el servicio «%s»: código de error %d\n"

#: pg_ctl.c:1172
#, c-format
msgid "%s: could not start service \"%s\": error code %d\n"
msgstr "%s: no se pudo iniciar el servicio «%s»: código de error %d\n"

#: pg_ctl.c:1181
#, c-format
msgid "Try \"%s --help\" for more information.\n"
msgstr "Use «%s --help» para obtener más información.\n"

#: pg_ctl.c:1189
#, c-format
msgid ""
"%s is a utility to start, stop, restart, reload configuration files,\n"
"report the status of a PostgreSQL server, or signal a PostgreSQL process.\n"
"\n"
msgstr ""
"%s es un programa para iniciar, detener, reiniciar, recargar archivos de\n"
"configuración, reportar el estado de un servidor PostgreSQL o enviar una\n"
"señal a un proceso PostgreSQL.\n"
"\n"

#: pg_ctl.c:1191
#, c-format
msgid "Usage:\n"
msgstr "Empleo:\n"

#: pg_ctl.c:1192
#, c-format
msgid "  %s start   [-w] [-D DATADIR] [-s] [-l FILENAME] [-o \"OPTIONS\"]\n"
msgstr "  %s start   [-w] [-D DATADIR] [-s] [-l ARCHIVO] [-o «OPCIONES»]\n"

#: pg_ctl.c:1193
#, c-format
msgid "  %s stop    [-W] [-D DATADIR] [-s] [-m SHUTDOWN-MODE]\n"
msgstr "  %s stop    [-W] [-D DATADIR] [-s] [-m MODO-DETENCIÓN]\n"

#: pg_ctl.c:1194
#, c-format
msgid ""
"  %s restart [-w] [-D DATADIR] [-s] [-m SHUTDOWN-MODE] [-o \"OPTIONS\"]\n"
msgstr ""
"  %s restart [-w] [-D DATADIR] [-s] [-m MODO-DETENCIÓN] [-o «OPCIONES»]\n"

#: pg_ctl.c:1195
#, c-format
msgid "  %s reload  [-D DATADIR] [-s]\n"
msgstr "  %s reload  [-D DATADIR] [-s]\n"

#: pg_ctl.c:1196
#, c-format
msgid "  %s status  [-D DATADIR]\n"
msgstr "  %s status  [-D DATADIR]\n"

#: pg_ctl.c:1197
#, c-format
msgid "  %s kill    SIGNALNAME PID\n"
msgstr "  %s kill    NOMBRE-SEÑAL ID-DE-PROCESO\n"

#: pg_ctl.c:1199
#, c-format
msgid ""
"  %s register   [-N SERVICENAME] [-U USERNAME] [-P PASSWORD] [-D DATADIR]\n"
"                    [-w] [-o \"OPTIONS\"]\n"
msgstr ""
"  %s register   [-N SERVICIO] [-U USUARIO] [-P PASSWORD] [-D DATADIR]\n"
"                    [-w] [-o «OPCIONES»]\n"

#: pg_ctl.c:1201
#, c-format
msgid "  %s unregister [-N SERVICENAME]\n"
msgstr "  %s unregister [-N SERVICIO]\n"

#: pg_ctl.c:1204
#, c-format
msgid ""
"\n"
"Common options:\n"
msgstr ""
"\n"
"Opciones comunes:\n"

#: pg_ctl.c:1205
#, c-format
msgid "  -D, --pgdata DATADIR   location of the database storage area\n"
msgstr ""
"  -D, --pgdata DATADIR   ubicación del área de almacenamiento de datos\n"

#: pg_ctl.c:1206
#, c-format
msgid "  -s, --silent           only print errors, no informational messages\n"
msgstr ""
"  -s, --silent           mostrar sólo errores, no mensajes de información\n"

#: pg_ctl.c:1207
#, c-format
msgid "  -w                     wait until operation completes\n"
msgstr ""
"  -w                     esperar hasta que la operación se haya completado\n"

#: pg_ctl.c:1208
#, c-format
msgid "  -W                     do not wait until operation completes\n"
msgstr ""
"  -W                     no esperar hasta que la operación se haya "
"completado\n"

#: pg_ctl.c:1209
#, c-format
msgid "  --help                 show this help, then exit\n"
msgstr "  --help                 mostrar este texto y salir\n"

#: pg_ctl.c:1210
#, c-format
msgid "  --version              output version information, then exit\n"
msgstr "  --version              mostrar información sobre versión y salir\n"

#: pg_ctl.c:1211
#, c-format
msgid ""
"(The default is to wait for shutdown, but not for start or restart.)\n"
"\n"
msgstr ""
"(Por omisión se espera para las detenciones, pero no los inicios o "
"reinicios)\n"
"\n"

#: pg_ctl.c:1212
#, c-format
msgid "If the -D option is omitted, the environment variable PGDATA is used.\n"
msgstr "Si la opción -D es omitida, se usa la variable de ambiente PGDATA.\n"

#: pg_ctl.c:1214
#, c-format
msgid ""
"\n"
"Options for start or restart:\n"
msgstr ""
"\n"
"Opciones para inicio y reinicio:\n"

#: pg_ctl.c:1215
#, c-format
msgid "  -l, --log FILENAME     write (or append) server log to FILENAME\n"
msgstr ""
"  -l  --log ARCHIVO      guardar el registro del servidor en ARCHIVO.\n"

#: pg_ctl.c:1216
#, c-format
msgid ""
"  -o OPTIONS             command line options to pass to the postmaster\n"
"                         (PostgreSQL server executable)\n"
msgstr ""
"  -o OPCIONES            parámetros de línea de órdenes a pasar a "
"postmaster\n"
"                         (ejecutable del servidor de PostgreSQL)\n"

#: pg_ctl.c:1218
#, c-format
msgid "  -p PATH-TO-POSTMASTER  normally not necessary\n"
msgstr "  -p RUTA-A-POSTMASTER   normalmente no es necesario\n"

#: pg_ctl.c:1220
#, c-format
msgid ""
"\n"
"Options for stop or restart:\n"
msgstr ""
"\n"
"Opciones para detención y reinicio:\n"

#: pg_ctl.c:1221
#, c-format
msgid "  -m SHUTDOWN-MODE   may be \"smart\", \"fast\", or \"immediate\"\n"
msgstr "  -m MODO-DE-DETENCIÓN   puede ser «smart», «fast» o «immediate»\n"

#: pg_ctl.c:1223
#, c-format
msgid ""
"\n"
"Shutdown modes are:\n"
msgstr ""
"\n"
"Modos de detención son:\n"

#: pg_ctl.c:1224
#, c-format
msgid "  smart       quit after all clients have disconnected\n"
msgstr ""
"  smart       salir después que todos los clientes se hayan desconectado\n"

#: pg_ctl.c:1225
#, c-format
msgid "  fast        quit directly, with proper shutdown\n"
msgstr "  fast        salir directamente, con apagado apropiado\n"

#: pg_ctl.c:1226
#, c-format
msgid ""
"  immediate   quit without complete shutdown; will lead to recovery on "
"restart\n"
msgstr ""
"  immediate   salir sin apagado completo; se ejecutará recuperación\n"
"              en el próximo inicio\n"
"\n"

#: pg_ctl.c:1228
#, c-format
msgid ""
"\n"
"Allowed signal names for kill:\n"
msgstr ""
"\n"
"Nombres de señales permitidos para kill:\n"

#: pg_ctl.c:1232
#, c-format
msgid ""
"\n"
"Options for register and unregister:\n"
msgstr ""
"\n"
"Opciones para registrar y dar de baja:\n"

#: pg_ctl.c:1233
#, c-format
msgid ""
"  -N SERVICENAME  service name with which to register PostgreSQL server\n"
msgstr ""
"  -N SERVICIO            nombre de servicio con el cual registrar\n"
"                         el servidor PostgreSQL\n"

#: pg_ctl.c:1234
#, c-format
msgid "  -P PASSWORD     password of account to register PostgreSQL server\n"
msgstr ""
"  -P CONTRASEÑA          contraseña de la cuenta con la cual registrar\n"
"                         el servidor PostgreSQL\n"

#: pg_ctl.c:1235
#, c-format
msgid "  -U USERNAME     user name of account to register PostgreSQL server\n"
msgstr ""
"  -U USUARIO             nombre de usuario de la cuenta con la cual\n"
"                         registrar el servidor PostgreSQL\n"

#: pg_ctl.c:1238
#, c-format
msgid ""
"\n"
"Report bugs to <pgsql-bugs@postgresql.org>.\n"
msgstr ""
"\n"
"Reporte errores a <pgsql-bugs@postgresql.org>.\n"

#: pg_ctl.c:1263
#, c-format
msgid "%s: unrecognized shutdown mode \"%s\"\n"
msgstr "%s: modo de apagado «%s» no reconocido\n"

#: pg_ctl.c:1296
#, c-format
msgid "%s: unrecognized signal name \"%s\"\n"
msgstr "%s: nombre de señal «%s» no reconocido\n"

#: pg_ctl.c:1360
#, c-format
msgid ""
"%s: cannot be run as root\n"
"Please log in (using, e.g., \"su\") as the (unprivileged) user that will\n"
"own the server process.\n"
msgstr ""
"%s: no puede ser ejecutado como root\n"
"Por favor conéctese (por ej. usando «su») con un usuario no privilegiado,\n"
"quien ejecutará el proceso servidor.\n"

#: pg_ctl.c:1464
#, c-format
msgid "%s: too many command-line arguments (first is \"%s\")\n"
msgstr "%s: demasiados argumentos de línea de comandos (el primero es «%s»)\n"

#: pg_ctl.c:1483
#, c-format
msgid "%s: missing arguments for kill mode\n"
msgstr "%s: argumentos faltantes para envío de señal\n"

#: pg_ctl.c:1501
#, c-format
msgid "%s: unrecognized operation mode \"%s\"\n"
msgstr "%s: modo de operación «%s» no reconocido\n"

#: pg_ctl.c:1511
#, c-format
msgid "%s: no operation specified\n"
msgstr "%s: no se especificó operación\n"

#: pg_ctl.c:1527
#, c-format
msgid ""
"%s: no database directory specified and environment variable PGDATA unset\n"
msgstr ""
"%s: no se especificó directorio de datos y la variable PGDATA no está "
"definida\n"

