#, c-format
msgid ""
"  -T, --tablespace-mapping=OLDDIR=NEWDIR\n"
"                         relocate tablespace in OLDDIR to NEWDIR\n"
msgstr ""
"  -T, --tablespace-mapping=GAMMALKAT=NYKAT\n"
"                         flytta tablespace i GAMMALKAT till NYKAT\n"

