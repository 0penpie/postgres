msgstr "  -z, --gzip             komprimera tar-utdata\n"

#: pg_basebackup.c:257
#, c-format
msgid "  -Z, --compress=0-9     compress tar output with given compression level\n"
msgstr "  -Z, --compress=0-9     komprimera tar-utdata med given komprimeringsnivå\n"

